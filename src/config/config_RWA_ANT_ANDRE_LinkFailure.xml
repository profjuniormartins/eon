	<!-- The simulator's main part. -->
	<Main>
		<!-- The variable of the simulation -->
		<Variable name="/Generators/Traffic/@load" step="10" stop="100.1"/>
		<!-- Number of requests. -->
		<Requests value="100000"/>
		<!-- The control plane used for this simulation. -->
		<!-- <ControlPlane class="main.ControlPlane"/> -->
		<ControlPlane class="rwa.crankback.antnetInter.AntNetCrankInterControlPlane" seed="666"/>	
	</Main>
	<!-- The generators of traffic for the simulation -->
	<Generators>
		<!-- Traffic type. -->	
		<!-- Traffic class="event.EventSubscriber" type="distribution.Distribution" averagePacket="length(bytes)" load="total load network" dataRate="(bits/sec)" seed="random seed"/> -->
	
		<Traffic class="rwa.AdaptativeIntraLightpathTraffic" type="Poissonian" start="1000" load="30" duration="100" seed="777"/>
		<Traffic class="rwa.InterLightpathTraffic" type="Poissonian" start="1000" load="30" duration="100" seed="555"/> 
		<Traffic intraSeed="555"/> 	
		<Traffic class="rwa.crankback.antnetInter.InterAntTraffic" type="Constant" start="0" serviceRate="1.0" arrivalRate="100"/> 
		 <Traffic class="rwa.crankback.antnetInter.AntTraffic" type="Constant" start="0" serviceRate="1.0" arrivalRate="300"/> 
	</Generators>
	<!-- Accounting part. -->	
	<Accounting>
		<!-- The accounting class -->
		<!-- <Accounting class="main.SimulationAccounting"/>-->
		<Accounting class="rwa.crankback.antnetInter.CrankAccounting"/>
		<!-- The subscribers that are accounted for the number of requests. -->
		<!-- <RequestRelated class="event.EventSubscriber"/> -->
		<RequestRelated class="rwa.InterLightpathTraffic"/>	
		<!--  PARA INTRA -->
		 <RequestRelated class="rwa.AdaptativeIntraLightpathTraffic"/> 	
	</Accounting>	
	<OPS>
		<!-- Hop limit="maximum number of hops" bytes="number of bytes of each node identifier"-->
		<Hop limit="60" bytes="6"/>
	</OPS>
	<!-- Configuration for ant routing -->
	<Ant>
		<Parametric factor="0.005" reductor="0.3"/>
		<Routing correction="0.6" power="4"/>
		<Pheromone confidence="0.65" firstWeight="0.6" secondWeight="0.4" amplifier="5"/>
		<Seed value="150889"/>
		<Holdoff timer="0.2" antRate="1E3"/>
	</Ant>	
	<!-- Configuration for RWA. -->
	<RWA>
		<Routing deterministic="true" rerouting="SEGMENT" attempts="1" maxAttempts="1" interAttempts="1" interRoutes="3"/>
		<WA type="FIRST_FIT" wavelength="8" window="10" sliding="true"/>
		<Serialize rt="false" file="tables.obj"/>
	</RWA>
	<!-- Configuration for the output. -->
	<Outputs>
		<!-- The output file containing the simulation values. -->
		<Output file="$out$"/>
		<!-- The transient time slice. -->
		<Transient timeSlice="1E10"/>
		<!-- The variables that are printed in the output file. -->
		<Print>LOAD</Print>
		<Print>LOSS</Print>
<Print>RESTORABILITY</Print>
		<!-- In other files. -->
		<Print>NODE</Print> 
		<Print>LINK</Print> 
		<Print>CLASS</Print>
		<Print>HOP</Print>
	</Outputs>
	<!-- Failure Simulation-->
	<Failure>
		  <LinkFailure link="$src$-$dst$" time="$time$"/>
		<LinkFailure link="$dst$-$src$" time="$time$"/> 

		<Timing localization="10E-3"/>
	</Failure>
<Terminate time="$time$" offset="100"/>
</Simulation>	 
